{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "name": "courses",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "beginnerClassesDayOfTheWeek": {
      "type": "string"
    },
    "intermediateClassesDayOfTheWeek": {
      "type": "string"
    },
    "advancedClassesDayOfTheWeek": {
      "type": "string"
    },
    "subTitle": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "enumeration",
      "enum": [
        "Nigeria",
        "Canterbury",
        "Online"
      ],
      "required": true
    },
    "courseType": {
      "type": "enumeration",
      "enum": [
        "Online",
        "Onsite"
      ],
      "required": true
    },
    "whatYouLearn": {
      "type": "richtext"
    },
    "materialsNeeded": {
      "type": "richtext"
    },
    "hasClassStage": {
      "type": "boolean"
    },
    "classesDayOfTheWeek": {
      "type": "string"
    },
    "paymentInformation": {
      "type": "richtext"
    },
    "startTime": {
      "type": "string"
    },
    "endTime": {
      "type": "string"
    },
    "PaymentOptions": {
      "type": "json",
      "required": true
    },
    "courseLength": {
      "type": "enumeration",
      "enum": [
        "long",
        "short"
      ],
      "default": "long",
      "required": true
    },
    "imageUrl": {
      "type": "string"
    },
    "region": {
      "type": "enumeration",
      "enum": [
        "Abuja",
        "Ibadan",
        "Canterbury",
        "Online"
      ],
      "required": true
    }
  }
}
